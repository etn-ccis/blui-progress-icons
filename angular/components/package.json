{
  "name": "@brightlayer-ui/ng-progress-icons",
  "version": "3.0.0-beta.0",
  "description": "Angular progress icons for Brightlayer UI applications",
  "main": "bundles/brightlayer-ui-ng-progress-icons.umd.js",
  "scripts": {
    "build": "rm -rf ../dist && yarn && ng build && cp ../README.md ../dist",
    "test": "ng test",
    "test:ci": "ng test --watch false",
    "prettier": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --write",
    "prettier:check": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --check",
    "lint": "eslint \"src/**/**.ts\"",
    "lint:fix": "eslint \"src/**/**.ts\" --fix",
    "publish:package": "cd ../dist && set npm_config_yes=true && npx -p @brightlayer-ui/publish blui-publish",
    "tag:package": "npx -p @brightlayer-ui/tag blui-tag -s -blui-ng-progress-icons"
  },
  "dependencies": {
    "color": "^3.1.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0",
    "@angular/animations": "^15.2.0",
    "@angular/cli": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@brightlayer-ui/eslint-config": "^3.0.1",
    "@brightlayer-ui/prettier-config": "^1.0.3",
    "@types/jasmine": "~3.9.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "core-js": "~2.5.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.5.1",
    "rxjs": "~7.5.6",
    "ts-node": "~10.2.1",
    "tslib": "^2.4.1",
    "typescript": "~4.8.4",
    "yarn-audit-fix": "^9.3.6",
    "zone.js": "~0.11.4"
  },
  "peerDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etn-ccis/blui-progress-icons.git"
  },
  "keywords": [
    "progress",
    "icons",
    "brightlayer-ui"
  ],
  "author": "Brightlayer UI <brightlayer-ui@eaton.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/etn-ccis/blui-progress-icons/issues"
  },
