    "description": "React progress icons for Brightlayer UI applications",
    "main": "./dist/index.js",
    "scripts": {
        "test": "jest",
        "build": "yarn && tsc",
        "lint": "eslint \"src/**/**.tsx\"",
        "lint:fix": "eslint \"src/**/**.tsx\" --fix",
        "prettier": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --write",
        "prettier:check": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --check",
        "publish:package": "set npm_config_yes=true && npx -p @brightlayer-ui/publish blui-publish",
        "tag:package": "npx -p @brightlayer-ui/tag blui-tag -s -blui-react-progress-icons"
    },
    "prettier": "@brightlayer-ui/prettier-config",
    "dependencies": {
        "clsx": "^1.1.1",
        "color": "^3.1.2",
        "react-jss": "^10.4.0"
    },
    "peerDependencies": {
        "react": ">=16.6.3",
        "react-dom": ">=16.8.6"
    },
    "devDependencies": {
        "@brightlayer-ui/eslint-config": "^3.0.1",
        "@brightlayer-ui/prettier-config": "^1.0.2",
        "@types/color": "^3.0.1",
        "@types/jest": "^26.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "babel-polyfill": "^6.26.0",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.20.6",
        "jest": "26.6.0",
        "prettier": "^2.0.0",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "ts-jest": "^26.0.0",
        "typescript": "^4.0.0"
    },
    "repository": {
        "type": "git",
